      <lg type="song lyrics">
        <head>Lady Jette</head>
        <lg number="1" type="stanza">
          <l>lady jette I'll be your sky </l>
          <l>come and breathe the morning air </l>
          <l>lady jette straight through the clouds </l>
          <l>counting sheep till we're in bed </l>
        </lg>
        <lg number="2" type="stanza">
          <l>lady jette so down to earth </l>
          <l>touch the ground with gentle care</l>
        </lg>
      </lg>
