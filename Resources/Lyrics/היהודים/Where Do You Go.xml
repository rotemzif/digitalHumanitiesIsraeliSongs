  <text>
    <body>
      <lg type="song lyrics">
        <head>Where Do You Go</head>
        <lg number="1" type="stanza">
          <l>please help me god</l>
          <l>please help me</l>
          <l>where do you go</l>
          <l>please tell me</l>
          <l>please tell me</l>
          <l>where do you go</l>
        </lg>
        <lg number="2" type="stanza">
          <l>i want to be with you</l>
          <l>i want to hold you even more</l>
          <l>where do you go</l>
          <l>i love you</l>
          <l>i want to let you know</l>
          <l>i love you</l>
          <l>i want you when you're gone</l>
          <l>where do you go</l>
        </lg>
        <lg number="3" type="stanza">
          <l>please take me now</l>
          <l>please make me</l>
          <l>where do you go</l>
          <l>please save me</l>
          <l>please have me</l>
          <l>where do you go</l>
        </lg>
        <lg number="4" type="stanza">
          <l>if i could be with you</l>
          <l>if i could see you just once more</l>
          <l>where do you go</l>
        </lg>
        <lg number="5" type="stanza">
          <l>i love you</l>
          <l>i want to let you go</l>
          <l>i love you</l>
          <l>i want you when you're gone</l>
          <l>where do you go </l>
        </lg>
      </lg>
    </body>
  </text>
