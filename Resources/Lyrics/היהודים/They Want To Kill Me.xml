  <text>
    <body>
      <lg type="song lyrics">
        <head>They Want To Kill Me</head>
        <lg number="1" type="stanza">
          <l>They want to kill me</l>
          <l>They want me bad</l>
          <l>Please don't hurt me</l>
          <l>Please don't hurt me</l>
          <l>Give me just one day</l>
          <l>To say good-bye to my lover</l>
        </lg>
        <lg number="2" type="stanza">
          <l>They want to feel me</l>
          <l>They want me bad</l>
          <l>Please don't hurt me</l>
          <l>Please don't hurt me</l>
          <l>Give me some silence</l>
          <l>One chance to fly</l>
          <l>to my lover</l>
        </lg>
        <lg number="3" type="stanza">
          <l>Cause i feel so low</l>
          <l>Cause i feel</l>
          <l>I feel so lonely</l>
        </lg>
        <lg number="4" type="stanza">
          <l>Don't need to feel the pain</l>
          <l>Don't need to cry</l>
          <l>Please don't hurt me</l>
          <l>Please don't hurt me</l>
          <l>give me some silence</l>
          <l>One chance to fly</l>
          <l>to my lover </l>
        </lg>
      </lg>
