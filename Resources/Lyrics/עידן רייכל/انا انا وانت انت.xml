  <text>
    <body>
      <lg type="song lyrics">
        <head>انا انا وانت انت</head>
        <lg number="1" type="stanza">
          <l>انت انت </l>
          <l>وانا انا </l>
          <l>خليك انت </l>
          <l>واتركني </l>
        </lg>
        <lg number="2" type="stanza">
          <l>كل واحد ودنياته </l>
          <l>بقرر حياته </l>
          <l>انت حر بامرك </l>
          <l>الا اذا بتقيد فيي </l>
        </lg>
        <lg number="3" type="stanza">
          <l>انت انت </l>
          <l>وانا انا </l>
          <l>خليك انت </l>
          <l>واتركني </l>
          <l>انت انت (كون انت) </l>
          <l>وانا انا (بكون انا) </l>
          <l>خليك انت (كون انت) </l>
          <l>واتركني (اكون انا) </l>
        </lg>
        <lg number="4" type="stanza">
          <l>وحاجي تعيرني </l>
          <l>بدك تغيرني </l>
          <l>انا والله عال العال </l>
          <l>الا اذا بتقيد فيي </l>
        </lg>
        <lg number="5" type="stanza">
          <l>خليه بالاعتبار </l>
          <l>انه ولدنا احرار </l>
          <l>مثلي مثلك انسان </l>
          <l>بدي ابني كيان </l>
        </lg>
        <lg number="6" type="stanza">
          <l>انت انت</l>
        </lg>
      </lg>
    </body>
