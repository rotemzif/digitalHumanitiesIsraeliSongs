  <text>
    <body>
      <lg type="song lyrics">
        <head>יהודה יהודה</head>
        <lg Chorus="yes" number="1" type="stanza">
          <l>תן לו בעניבה</l>
          <l>תן לו בחליפה</l>
          <l>תן לו בחפתים</l>
          <l>ויהודה יהודה בא שלום</l>
        </lg>
        <lg Chorus="yes" number="2" type="stanza">
          <l>יהודה יהודה יא יהודה</l>
          <l>יהודה יהודה יא יהודה</l>
          <l>יהודה יהודה יא יהודה</l>
          <l>יהודה יהודה יא יהודה</l>
        </lg>
        <lg Chorus="yes" number="3" type="stanza">
          <l>תן לו במה נשמע</l>
          <l>תן לו ברוך הבא</l>
          <l>תן ביהודה</l>
          <l>ויהודה יהודה בשלום</l>
        </lg>
        <lg Chorus="yes" number="4" type="stanza">
          <l>יהודה יהודה...</l>
        </lg>
        <lg Chorus="yes" number="5" type="stanza">
          <l>תן לו במנעמים</l>
          <l>תן לו בליטופים</l>
          <l>תן לו באהבים</l>
          <l>ויהודה יהודה בא שלום</l>
        </lg>
        <lg Chorus="yes" number="6" type="stanza">
          <l>יהודה יהודה...</l>
        </lg>
      </lg>
    </body>
  </text>
